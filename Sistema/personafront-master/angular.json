            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "beneficio:build:production"
            },
            "development": {
              "buildTarget": "beneficio:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    },
    "peso-cabal": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/peso-cabal",
      "sourceRoot": "projects/peso-cabal/src",
